{
  "name": "redux-create-types",
  "version": "0.0.1",
  "description": "create types with namespacing",
  "main": "lib",
  "scripts": {
    "test": "nyc ava",
    "test:watch": "ava --watch",
    "lint": "eslint src test",
    "cover": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BerkeleyTrue/redux-create-types.git"
  },
  "keywords": [
    "react",
    "redux",
    "redux-actions",
    "types"
  ],
  "ava": {
    "require": [
      "babel-register"
    ],
    "failFast": true
  },
  "author": "Berkeley Martinez <Berkeley@RoboTie.com> (http://RoboTie.com)",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.20.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.1.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "coveralls": "^2.11.14",
    "eslint": "^4.1.0",
    "eslint-plugin-import": "^2.0.1",
    "nyc": "^11.0.1"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "invariant": "^2.2.1"
  }
}
